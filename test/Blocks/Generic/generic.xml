<?xml version="1.0" encoding="UTF-8"?>
<Blocks package="Generic" category="Generic">
	<Block name="Display" TypeName="GenericDisplay">
		<input name="Text" type="str" default="''" />
		<attrib name="Title" type="str" default="'Display'" />
		<output name="Show" type="int" />
	</Block>
	<Block name="If" TypeName="Generic">
		<input name="condition" type="any" default="False" />
		<input name="Tcallback" type="any" default="None" />
		<input name="Fcallback" type="any" default="None" />
		<output name="Result" type="any" callback="TrueOrFalse" />
	</Block>
	<Block name="Buffer" TypeName="Generic">
		<input name="Value" type="any" default="None" />
		<output name="Output" type="any" callback="generic_Buffer" />
	</Block>
	<Block name="UnitDelay" TypeName="DelayObject" >
		<attrib name="Initial" type="any" default="None" />
		<input name="Input" type="any" default="None" />
		<output name="Output" type="any" callback="generic_delay" />
	</Block>
</Blocks>
