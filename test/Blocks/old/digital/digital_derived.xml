<?xml version="1.0" encoding="UTF-8"?>
<blocks category="Digital Logic(Complex)">
	<block name="Logic.Derived.HalfAdder">
		<input name="A" type="bool" default="False" />
		<input name="B" type="bool" default="False" />
		<output name="sum" type="bool" callback="half_adder_sum" args="A,B" />
		<output name="carry" type="bool" callback="half_adder_carry" args="A,B" />
	</block>
	<block name="Logic.Derived.FullAdder">
		<input name="A" type="bool" default="False" />
		<input name="B" type="bool" default="False" />
		<input name="Cin" type="bool" default="False" />
		<output name="sum" type="bool" callback="full_adder_sum" args="A,B,Cin" />
		<output name="Cout" type="bool" callback="full_adder_carry" args="A,B,Cin" />
	</block>
</blocks>